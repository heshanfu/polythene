{
  "name": "polythene-mithril",
  "version": "1.0.0",
  "description": "",
  "main": "dist/polythene-mithril",
  "module": "dist/polythene-mithril.mjs",
  "scripts": {
    "clean": "rm -rf dist/*",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build-standalone": "DEPS=1 ENTRY=scripts/standalone.js SOURCEMAP=1 DEST=./dist/polythene-mithril-standalone ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd-includes.js",
    "build": "yarn run clean && yarn run rollup && yarn run build-standalone"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "mithril": "1.1.6",
    "polythene-css": "^1.0.0",
    "polythene-mithril-base": "^1.0.0",
    "polythene-mithril-button": "^1.0.0",
    "polythene-mithril-card": "^1.0.0",
    "polythene-mithril-checkbox": "^1.0.0",
    "polythene-mithril-dialog": "^1.0.0",
    "polythene-mithril-dialog-pane": "^1.0.0",
    "polythene-mithril-drawer": "^1.0.0",
    "polythene-mithril-fab": "^1.0.0",
    "polythene-mithril-icon": "^1.0.0",
    "polythene-mithril-icon-button": "^1.0.0",
    "polythene-mithril-icon-label-button": "^1.0.0",
    "polythene-mithril-ios-spinner": "^1.0.0",
    "polythene-mithril-list": "^1.0.0",
    "polythene-mithril-list-tile": "^1.0.0",
