    "polythene-mithril-shadow": "^1.0.0",
    "polythene-mithril-slider": "^1.0.0",
    "polythene-mithril-snackbar": "^1.0.0",
    "polythene-mithril-svg": "^1.0.0",
    "polythene-mithril-switch": "^1.0.0",
