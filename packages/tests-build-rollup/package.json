{
  "name": "polythene-build-rollup",
  "version": "1.1.0",
  "description": "",
  "private": true,
  "main": "dist/js/index",
  "module": "dist/js/index.mjs",
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "dev": "http-server -p 5000 dist",
    "rollup": "DEPS=1 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd-includes.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build": "yarn run clean && yarn run rollup"
  },
  "dependencies": {
    "mithril": "1.1.6",
    "polythene-css": "^1.1.0",
    "polythene-mithril": "^1.1.0"
  },
  "devDependencies": {
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-watch": "4.3.1",
    "rollup-webpack-loader": "^1.0.0"
  }
}
